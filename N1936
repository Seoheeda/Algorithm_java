import java.util.Scanner;
import java.io.FileInputStream;

class Solution
{
	public static void main(String args[]) throws Exception
	{
		Scanner sc = new Scanner(System.in);
        int a;
        int b;
        a = sc.nextInt();
		b = sc.nextInt();
        
        if (a == 1 & b == 2) {
            System.out.println("B");
        } else if (a == 2 & b == 3) {
            System.out.println("B");
        } else if (a == 3 & b == 1) {
            System.out.println("B");
        } else {
            System.out.println("A");
    }
}
}
